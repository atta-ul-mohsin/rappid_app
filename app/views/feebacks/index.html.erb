<p id="notice"><%= notice %></p>

<h1>Feebacks</h1>

<table>
  <thead>
    <tr>
      <th>Grade</th>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feebacks.each do |feeback| %>
      <tr>
        <td><%= feeback.grade %></td>
        <td><%= feeback.content %></td>
        <td><%= feeback.user_id %></td>
        <td><%= link_to 'Show', feeback %></td>
        <td><%= link_to 'Edit', edit_feeback_path(feeback) %></td>
        <td><%= link_to 'Destroy', feeback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feeback', new_feeback_path %>
